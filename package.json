{
  "name": "smallorange-dynamodb-client",
  "version": "1.1.3",
  "description": "Lightweight dynamoDb client built on top of RxJS",
  "main": "index.js",
  "scripts": {
    "deploy": "git add --all; git commit -m 'Bump' && npm version patch -m 'Bump %s' && git push origin master && npm publish",
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha spec --recursive --watch",
    "testOnce": "NODE_ENV=test ./node_modules/.bin/_mocha spec --recursive --exit",
    "coverage": "NODE_ENV=test ./node_modules/.bin/istanbul cover -x 'spec/**' -x 'testing/**' ./node_modules/.bin/_mocha -- 'spec' --recursive"
  },
  "author": "Felipe Rohde",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.112.0",
    "cuid": "^1.3.8",
    "lodash": "^4.17.4",
    "rxjs": "^5.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feliperohdee/smallorange-dynamodb-client"
  },
  "homepage": "https://smallorange.co",
  "devDependencies": {
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0"
  }
}
