{
    "name": "rxjs-dynamodb-client",
    "version": "1.0.7",
    "description": "Lightweight dynamoDb client built on top of RxJS",
    "main": "index.js",
    "scripts": {
        "coverage": "rm -rf coverage; NODE_ENV=test nyc ./node_modules/.bin/_mocha **.spec.js **/*.spec.js --recursive --timeout 5000; rm -rf .nyc_output",
        "eslint": "eslint . --ext .js --config node_modules/eslint-config/index.js",
        "test": "NODE_ENV=test ./node_modules/.bin/_mocha **.spec.js **/*.spec.js --recursive --watch --timeout 5000"
    },
    "author": "Felipe Rohde",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.568.0",
        "cuid": "^2.1.6",
        "lodash": "^4.17.15",
        "rxjs": "6.5.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/feliperohdee/rxjs-dynamodb-client"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "eslint": "^6.6.0",
        "mocha": "^5.1.1",
        "nyc": "^14.1.1",
        "sinon": "^4.5.0",
        "sinon-chai": "^3.0.0"
    },
    "nyc": {
        "all": true,
        "reporter": "lcov",
        "exclude": [
            "*.spec.js",
            "**/*.spec.js",
            "testing/**"
        ]
    }
}
